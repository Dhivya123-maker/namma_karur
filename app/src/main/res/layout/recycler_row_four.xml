<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">



    <LinearLayout
        android:id="@+id/ac_onee"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="5dp"
        android:padding="5dp"
        android:elevation="5dp"
        android:clipToPadding="false"
        android:background="@drawable/bg2"
        android:backgroundTint="#4c9a2a"
        android:layout_marginEnd="-10dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dp"
            android:orientation="vertical">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center"
                    android:background="@drawable/border"
                    android:backgroundTint="@color/white"
                    android:gravity="center">

                    <ImageView
                        android:id="@+id/sr_img"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_gravity="center"
                        >

                    </ImageView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/sr_txt_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="@color/white"
                        android:textSize="15dp">

                    </TextView>

                    <TextView
                        android:id="@+id/sr_txt_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:text=""
                        android:textColor="@color/white"
                        android:textSize="12dp">

                    </TextView>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="8dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/time_img"
                            android:layout_width="12dp"
                            android:layout_height="12dp"
                            android:layout_gravity="center"
                            android:src="@drawable/clock"/>
                        <TextView
                            android:id="@+id/time_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:layout_gravity="center"
                            android:textStyle="bold"
                            android:layout_marginStart="5dp"
                            android:textColor="@color/white"
                            android:textSize="12dp"/>


                    </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="end"
            android:layout_marginEnd="20dp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/distance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginTop="5dp"
                android:textStyle="bold"
                android:textColor="@color/white"
                android:textSize="12dp"/>

        </LinearLayout>
                </LinearLayout>



            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="0dp"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/sr_txt_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:text=""
                    android:textColor="@color/white"
                    android:textSize="12dp">

                </TextView>
                <ImageView

                    android:layout_width="60dp"
                    android:layout_height="20dp"
                    android:src="@drawable/five_starr"
                    android:layout_marginStart="5dp"

                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="10dp"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/verify_img"
                        android:layout_width="16dp"
                        android:layout_height="16dp"
                        android:src="@drawable/verified"/>
                    <TextView
                        android:id="@+id/verified_txt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textStyle="bold"
                        android:layout_marginStart="5dp"
                        android:textColor="@color/white"
                        android:textSize="12dp"/>




                </LinearLayout>




            </LinearLayout>








        </LinearLayout>
    </LinearLayout>








    <LinearLayout
        android:id="@+id/atm_lnr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="5dp"
        android:padding="5dp"
        android:elevation="5dp"
        android:clipToPadding="false"
        android:background="@drawable/bg2"
        android:backgroundTint="#4c9a2a"
        android:layout_marginEnd="-10dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dp"
            android:orientation="vertical">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center"
                    android:background="@drawable/border"
                    android:backgroundTint="@color/white"
                    android:gravity="center">

                    <ImageView
                      android:id="@+id/atm_img"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_gravity="center"
                        >

                    </ImageView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="5dp"
                    android:orientation="vertical">
                    <TextView
                       android:id="@+id/atm_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="@color/white"
                        android:textSize="15dp">

                    </TextView>

                    <TextView
                       android:id="@+id/atm_address"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:text=""
                        android:textColor="@color/white"
                        android:textSize="12dp">

                    </TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:orientation="horizontal">


                        <ImageView
                            android:id="@+id/img_loc"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/location"
                            android:layout_marginTop="5dp"/>
                        <TextView
                            android:id="@+id/locationn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginTop="5dp"
                            android:textColor="@color/white"
                            android:text=""/>

                    </LinearLayout>





                </LinearLayout>



            </LinearLayout>

<!--            <LinearLayout-->
<!--                android:layout_width="wrap_content"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:layout_marginStart="0dp"-->
<!--                android:orientation="horizontal">-->

<!--                <TextView-->
<!--                  android:id="@+id/atm_rate"-->
<!--                    android:layout_width="wrap_content"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:layout_marginStart="10dp"-->
<!--                    android:text=""-->
<!--                    android:textColor="@color/white"-->
<!--                    android:textSize="12dp">-->

<!--                </TextView>-->
<!--                <ImageView-->
<!--                    android:id="@+id/atm_star"-->
<!--                    android:layout_width="50dp"-->
<!--                    android:layout_height="20dp"-->
<!--                    android:src="@drawable/five_starr"-->
<!--                    android:layout_marginStart="5dp"-->

<!--                    />-->



<!--            </LinearLayout>-->








        </LinearLayout>
    </LinearLayout>
</LinearLayout>
